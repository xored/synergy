<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http:///com/xored/sherlock/sample/sources.ecore"
            class="com.xored.sherlock.sample.sources.SourcesPackage"
            genModel="model/sources.genmodel"/>
   </extension>
   <extension point="com.xored.sherlock.eclipse.core.sources">
      <source
            class="com.xored.sherlock.sample.sources.DateDataSource"
            eClass="http:///com/xored/sherlock/sample/sources.ecore#//Date"
            id="com.xored.sherlock.sample.date"/>
   </extension>
   <extension point="com.xored.sherlock.eclipse.core.sources">
      <source
            class="com.xored.sherlock.sample.sources.DurationDataSource"
            eClass="http:///com/xored/sherlock/sample/sources.ecore#//Duration"
            id="com.xored.sherlock.sample.duration"/>
   </extension>
   <extension point="com.xored.sherlock.eclipse.core.sources">
      <source
            class="com.xored.sherlock.sample.sources.SamplingDataSource"
            eClass="http:///com/xored/sherlock/sample/sources.ecore#//Sample"
            id="com.xored.sherlock.sample.sampling"/>
   </extension>

</plugin>

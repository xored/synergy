<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.views">
      <view
            class="com.xored.sherlock.sample.sources.ui.SampleSourcesView"
            id="com.xored.sherlock.sample.view"
            name="Sample Sherlock Sources"
            restorable="true"/>
   </extension>
   <extension point="com.xored.sherlock.eclipse.core.sources">
      <source
            class="com.xored.sherlock.sample.sources.ui.SampleEventSource"
            eClass="http:///com/xored/sherlock/sample/sources.ecore#//Sample"
            id="com.xored.sherlock.sample.event"/>
   </extension>
   <extension point="com.xored.sherlock.eclipse.core.sources">
      <source
            class="com.xored.sherlock.sample.sources.ui.SampleProcessSource"
            eClass="http:///com/xored/sherlock/sample/sources.ecore#//Sample"
            id="com.xored.sherlock.sample.process"/>
   </extension>

</plugin>
